    "@types/react-dom": "^17.0.3",
    "@types/react-transition-group": "^4.4.1",
    "@types/storybook__addon-info": "^5.2.3",
    "node-sass": "4.14.1",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "7.3.1",
